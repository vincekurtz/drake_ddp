<?xml version="1.0" ?>
<robot name="mini_cheetah" xmlns:xacro="http://ros.org/wiki/xacro">
    <!-- Trunk model (body + feet only) approximation of the mini cheetah robot -->

    <!--!!!!!!!!!!!!!!!!!!BODY!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!-->
    <link name="body">
        <inertial>
            <mass value="3.3"/>
            <origin xyz="0.0 0.0 0.0"/>
            <inertia ixx="0.011253" ixy="0" ixz="0" iyy="0.036203" iyz="0" izz="0.042673"/>
        </inertial>
        <visual>
            <geometry>
                <box size="0.4 0.19 0.1"/>
            </geometry>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.23 0.19 0.1"/>
            </geometry>
            <drake:proximity_properties>
                <drake:compliant_hydroelastic/>
                <drake:mesh_resolution_hint value="0.05"/>
                <drake:hydroelastic_modulus value="1e4"/>
                <drake:hunt_crossley_dissipation value="1.25"/>
            </drake:proximity_properties>
            <origin rpy="0 0 0" xyz="0 0 0"/>
        </collision>
    </link>

    <!--!!!!!!!!!!!!!!!! LEFT FRONT FOOT !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!-->
    <joint name="fl_x" type="prismatic">
        <parent link="body"/>
        <child link="LF_FOOT"/>
        <origin rpy="0.0 0.0 0.0" xyz="0.2 0.1 -0.2"/>
            <!--<limit effort="20" lower="-0.1" upper="0.1" velocity="40"/>-->
    </joint>
    <link name="LF_FOOT">
        <collision>
            <geometry>
                <sphere radius = "0.0175"/>
            </geometry>
            <drake:proximity_properties>
                <drake:rigid_hydroelastic/>
                <drake:mesh_resolution_hint value="0.02"/>
                <drake:hunt_crossley_dissipation value="1.25"/>
            </drake:proximity_properties>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        </collision>
        <visual>
            <geometry>
                <sphere radius = "0.0175"/>
            </geometry>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>
        </visual>
    </link>


    <!--!!!!!!!!!!!!!!!! Actuators !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!! -->
    <transmission name="fl_x_transmission">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="fl_x" />
        <actuator name="fl_x_actuator"/>
    </transmission>
    
</robot>
